// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://admin:admin@localhost:5432/database"
}

model Game {
  id        String @id 
  title     String
  bannerUrl String
}
